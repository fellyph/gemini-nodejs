{
    "name": "gemini-nodejs",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/app.js",
        "dev": "node --import ./register.js src/app.ts",
        "watch": "tsc -w",
        "demo0": "node --import ./register.js src/demos/demo-0-hello-world.ts",
        "demo1": "node --import ./register.js src/demos/demo-1-prompt-configuration.ts",
        "demo2": "node --import ./register.js src/demos/demo-2-streaming.ts",
        "demo3": "node --import ./register.js src/demos/demo-3-chat.ts",
        "demo4": "node --import ./register.js src/demos/demo-4-structured-output.ts",
        "demo5": "node --import ./register.js src/demos/demo-5-function-calling.ts",
        "demo6": "node --import ./register.js src/demos/demo-6-context-caching.ts",
        "demo7": "node --import ./register.js src/demos/demo-7-image-analysis.ts",
        "final:demo0": "node --import ./register.js src/final/demo-0-hello-world.ts",
        "final:demo1": "node --import ./register.js src/final/demo-1-prompt-configuration.ts",
        "final:demo2": "node --import ./register.js src/final/demo-2-streaming.ts",
        "final:demo3": "node --import ./register.js src/final/demo-3-chat.ts",
        "final:demo4": "node --import ./register.js src/final/demo-4-structured-output.ts",
        "final:demo5": "node --import ./register.js src/final/demo-5-function-calling.ts",
        "final:demo6": "node --import ./register.js src/final/demo-6-context-caching.ts",
        "final:demo7": "node --import ./register.js src/final/demo-7-image-analysis.ts",
        "final:demo8": "node --import ./register.js src/final/demo-8-pdf-analyzer.ts",
        "final:demo9": "node --import ./register.js src/final/demo-9-image-generation.ts",
        "final:demo10": "node --import ./register.js src/final/demo-10-video-generation.ts",
        "final:demo11": "node --import ./register.js src/final/demo-11-prompt-thinking-configuration.ts",
        "pdf": "node --import ./register.js src/app-pdf.ts",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
        "format": "prettier --write \"src/**/*.{ts,ejs}\"",
        "format:check": "prettier --check \"src/**/*.{ts,ejs}\""
    },
    "dependencies": {
        "@google/genai": "^0.9.0",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.10",
        "express": "^4.18.3",
        "express-ejs-layouts": "^2.5.1",
        "fs": "^0.0.1-security",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/express-ejs-layouts": "^2.5.4",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.24",
        "ejs-lint": "^2.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "prettier-plugin-ejs": "^1.0.3",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
